{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "global-require": 0,
        "import/extensions": 0,
        "import/no-dynamic-require": 0,
        "import/no-extraneous-dependencies": 0,
        "import/no-named-as-default": 0,
        "import/no-unresolved": 0,
        "import/prefer-default-export": 0,
        "no-unused-vars": [
            2,
            {
                "args": "none"
            }
        ],
        "prettier/prettier": ["error", {
            "singleQuote": true,
            "trailingComma": "all"
        }],
        "react/jsx-indent": 0
    }
}